        <activity android:name=".actitvity.login.LogIn" />
        <activity android:name=".actitvity.MainActivity" />
        <activity android:name=".introScrean.InteroScrean">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".service" />
    </application>

</manifest>